{
  "name": "Glowpoll",
  "version": "0.1.0",
  "description": "Providing polling data with vote distribution over US.",
  "main": "src/server/server.ts",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && ts-node src/server/server.ts",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "ts-node-dev --respawn --transpileOnly src/server/server.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "tsc": "tsc",
    "heroku-prebuild": "npm install --dev"
  },
  "author": "Ruili Gao / Ziqi Dai",
  "license": "Do not distribute",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/mysql": "^2.15.6",
    "babel-polyfill": "^6.26.0",
    "datamaps": "^0.5.9",
    "express": "^4.16.4",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.0.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.5",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
